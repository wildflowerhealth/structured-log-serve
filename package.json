        "mongodb": "^3.0.10",
        "restify": "^7.2.1",
        "socket.io": "^2.1.1"
    },
    "devDependencies": {
        "@types/mongodb": "^3.0.19",
        "@types/restify": "^7.2.0",
        "@types/socket.io": "^1.4.34",
